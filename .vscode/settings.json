{
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features",
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[git-commit]": {
    "editor.rulers": [
      72,
      50
    ],
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.fontFamily": "monospace"
  },
  "[django-html]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.suggest.insertMode": "replace"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.markdown-language-features",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[pip-requirements]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.insertFinalNewline": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true
  },
  "[plaintext]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": false,
      "strings": false,
      "other": false
    },
    "editor.tabSize": 4,
    "files.insertFinalNewline": true
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    },
    "editor.rulers": [
      88,
      100
    ],
    "editor.wordBasedSuggestions": "off"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnSave": true,
    "files.eol": "\n"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "autopep8.args": [
    "--max-line-length=88",
    "--experimental",
    "--aggressive",
    "--aggressive",
    "--in-place",
    "--select=E,W,F",
    "--ignore=E226,E302,E41",
    "--exclude=.git,__pycache__,.pytest_cache,.venv,node_modules"
  ],
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    79,
    88,
    100
  ],
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "files.watcherInclude": [
    "**/*.py",
    "**/*.html",
    "**/*.css",
    "**/*.js"
  ],
  "extensions.ignoreRecommendations": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.indexing": true,
  "javascript.suggest.enabled": false,
  "typescript.suggest.enabled": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.advanced": {
    "listCount": 10,
    "inlineSuggestCount": 3,
    "debug.enable": false
  },
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "html.format.templating": true,
  "task.reconnection": true,
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.enablePreview": false,
  "debug.inlineValues": "auto",
  "debug.console.fontSize": 14,
  "debug.toolBarLocation": "docked",
  "debugpy.debugJustMyCode": false,
  "editor.stickyScroll.enabled": true,
  "eslint.validate": [
    "javascript"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "python.analysis.aiCodeActions": {
    "enabled": true
  },
  "python.analysis.generateWithTypeAnnotation": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "python.analysis.autoIndent": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.supportDocstringTemplate": true,
  "python.envFile": "${workspaceFolder}/.env",
  "python.globalModuleInstallation": false,
  "python.languageServer": "Pylance",
  "eslint.useFlatConfig": false,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.activateEnvironment": true,
  "python.terminal.shellIntegration.enabled": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.promptToConfigure": true,
  "html.autoClosingTags": true,
  "html.format.indentInnerHtml": true,
  "html.format.indentHandlebars": true,
  "html.hover.references": true,
  "html.suggest.html5": true,
  "css.hover.references": true,
  "css.lint.duplicateProperties": "error",
  "css.lint.emptyRules": "error",
  "css.lint.float": "error",
  "css.lint.idSelector": "error",
  "css.lint.boxModel": "error",
  "css.lint.hexColorLength": "error",
  "css.validate": true,
  "javascript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.completeJSDocs": true,
  "javascript.validate.enable": true,
  "eslint.enable": true,
  "eslint.lintTask.options": "/workspaces/greenova/.eslintrc.js",
  "eslint.probe": [
    "javascript"
  ],
  "json.validate.enable": true,
  "django.snippets.use": true,
  "eslint.run": "onSave",
  "files.associations": {
    "*.html": "django-html",
    "*.js": "javascript",
    "*.py": "python",
    "*.css": "css",
    "*.json": "json",
    "*.md": "markdown",
    "*.txt": "plaintext",
    "requirements*.txt": "pip-requirements",
    "*.toml": "toml",
    "pyproject.toml": "toml",
    "poetry.toml": "toml",
    "pdm.toml": "toml"
  },
  "html.format.wrapLineLength": 88,
  "html.format.wrapAttributes": "preserve-aligned",
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.extraPaths": [
    "${workspaceFolder}/.venv/lib/python3.9/site-packages",
    "${workspaceFolder}/greenova"
  ],
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.inlayHints.functionReturnTypes": false,
  "python.analysis.inlayHints.variableTypes": false,
  "python.testing.unittestEnabled": true,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./greenova",
    "-p",
    "*test*.py"
  ],
  "python.testing.pytestEnabled": false,
  "github.gitProtocol": "ssh",
  "python.analysis.autoFormatStrings": true,
  "html.format.enable": true,
  "css.format.enable": true,
  "javascript.format.enable": true,
  "json.format.enable": true,
  "markdown.validate.enabled": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.enablePytestSupport": false,
  "python.analysis.languageServerMode": "full",
  "python.analysis.logLevel": "Error",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingModuleSource": "none",
    "reportMissingTypeStubs": "warning",
    "reportUnknownMemberType": "warning",
    "reportUnknownArgumentType": "warning",
    "reportUnknownVariableType": "warning",
    "reportUnknownLambdaType": "warning",
    "reportUnusedCoroutine": "warning",
    "reportUntypedFunctionDecorator": "warning",
    "reportUnknownParameterType": "warning",
    "reportPrivateUsage": "warning",
    "reportGeneralTypeIssues": "error"
  },
  "python.analysis.importFormat": "absolute",
  "python.analysis.packageIndexDepths": [
    {
      "name": "django",
      "depth": 2
    }
  ],
  "python.analysis.stubPath": "${workspaceFolder}/.venv/lib/python3.9/site-packages/django-stubs",
  "python.analysis.exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.venv",
    "**/node_modules",
    "**/.vscode-server"
  ],
  "github.copilot.editor.enableCodeActions": true
}
