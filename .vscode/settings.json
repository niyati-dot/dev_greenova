{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[git-commit]": {
    "editor.defaultFormatter": "vscode.git",
    "editor.rulers": [72, 50],
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.fontFamily": "monospace"
  },
  "[django-html]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "monosans.djlint",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.suggest.insertMode": "replace"
  },
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[dockerfile]": {
    "editor.quickSuggestions": {
      "strings": true
    }
  },
  "[shellcheckrc]": {
    "editor.defaultFormatter": "timonwong.shellcheck",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[editorconfig]": {
    "editor.defaultFormatter": "EditorConfig.EditorConfig",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[pip-requirements]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.insertFinalNewline": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true
  },
  "[plaintext]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": false,
      "strings": false,
      "other": false
    },
    "editor.tabSize": 4,
    "files.insertFinalNewline": true
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    },
    "editor.rulers": [88, 100],
    "editor.wordBasedSuggestions": "off"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnSave": true,
    "files.eol": "\n"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[dotenv]": {
    "editor.defaultFormatter": "dotenv.dotenv-vscode",
    "editor.formatOnSave": true,
    "files.eol": "\n"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnSave": true,
    "files.eol": "\n"
  },
  "[makefile]": {
    "editor.defaultFormatter": "ms-vscode.makefile-tools",
    "editor.insertSpaces": false
  },
  "autopep8.args": [
    "--max-line-length=88",
    "--experimental",
    "--aggressive",
    "--aggressive",
    "--in-place",
    "--select=E,W,F",
    "--ignore=E226,E302,E41",
    "--exclude=.git,__pycache__,.pytest_cache,.venv,node_modules"
  ],
