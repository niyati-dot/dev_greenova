{
  "dependencies": {
    "@picocss/pico": "^2.1.1",
    "@types/gsap": "3.0.0",
    "cssnano-preset-advanced": "7.0.7",
    "gsap": "3.13.0",
    "htmx-ext-class-tools": "2.0.2",
    "htmx-ext-head-support": "2.0.4",
    "htmx-ext-loading-states": "2.0.1",
    "htmx-ext-path-deps": "2.0.1",
    "htmx.org": "^1.9.12",
    "hyperscript.org": "^0.9.14",
    "tailwindcss": "^4.1.5"
  },
  "scripts": {
    "lint": "eslint 'greenova/static/ts/src/**/*.{js,ts}'",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "stylelint '**/*.css' --fix",
    "test": "npm run lint && npm run stylelint",
    "css:build": "node scripts/build-postcss.js",
    "css:watch": "nodemon --watch greenova/static/scss -e scss -x 'npm run css:build'",
    "build:typescript": "node scripts/build-typescript.js",
    "watch:typescript": "nodemon --watch greenova/static/ts/src -e ts --exec \"npm run build:typescript\"",
    "build:assemblyscript": "node scripts/build-assemblyscript.js",
    "watch:assemblyscript": "nodemon --watch greenova/static/as/assembly -e ts --exec \"npm run build:assemblyscript\"",
    "build:all": "npm run build:assemblyscript && npm run build:typescript && npm run css:build",
    "update-check": "npx npm-check-updates",
    "update-packages": "npx npm-check-updates -u && npm install",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf greenova/static/ts/dist/*"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.26.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.15.16",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "assemblyscript": "0.27.36",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "esbuild": "^0.25.4",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "10.1.3",
    "eslint-config-stylelint": "24.0.0",
    "eslint-plugin-prettier": "5.4.0",
    "figma-developer-mcp": "0.2.2",
    "globals": "16.1.0",
    "nodemon": "^3.1.10",
    "postcss": "8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-combine-duplicated-selectors": "10.0.3",
    "postcss-font-magician": "^4.0.0",
    "postcss-import": "16.1.0",
    "postcss-nested": "7.0.2",
    "postcss-preset-env": "10.1.6",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "3.5.3",
    "protolint": "0.54.0",
    "rimraf": "^6.0.1",
    "sass": "^1.87.0",
    "stylelint": "16.19.1",
    "stylelint-config-recommended": "16.0.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-fluid-type": "^2.0.7",
    "tailwindcss-logical": "^4.0.0",
    "tailwindcss-opentype": "^1.1.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.32.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": "20.19.1",
    "npm": "11.3.0"
  },
  "name": "greenova",
  "version": "0.0.6",
  "description": "Greenova - Environmental Management Application",
  "packageManager": "npm@11.3.0",
  "private": true,
  "license": "AGPL-3.0"
}
